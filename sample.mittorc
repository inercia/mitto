acp:
  - auggie:
      command: auggie --acp
  - claude-code:
      command: npx -y @zed-industries/claude-code-acp@latest

web:
  host: 127.0.0.1  # Use "0.0.0.0" to listen on all interfaces
  port: 8080
  prompts:
    - name: "Continue"
      prompt: "Please continue with the current task."
    - name: "Propose a plan"
      prompt: "Please propose a plan for the current task."
